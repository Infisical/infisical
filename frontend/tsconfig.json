{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/components/*": [
        "components/*"
      ],
      "~/utilities/*": [
        "components/utilities/*"
      ],
      "~/*": [
        "const"
      ],
      "~/pages/*": [
        "pages/*"
      ]
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "components/basic/layout.js"  ],
  "exclude": [
    "node_modules"
  ]
}
